{
  "name": "@neuctra/authix",
  "version": "1.1.24",
  "description": "Universal authentication SDK and UI components for React (v16â€“v19) with TailwindCSS.",
  "author": "Taha Asif",
  "license": "MIT",
  "type": "module",
  "main": "./dist/neuctra-authix.umd.js",
  "module": "./dist/neuctra-authix.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/neuctra-authix.es.js",
      "require": "./dist/neuctra-authix.umd.js"
    },
    "./react": {
      "import": "./dist/react/index.es.js",
      "require": "./dist/react/index.umd.js"
    },
    "./sdk": {
      "import": "./dist/sdk/index.es.js",
      "require": "./dist/sdk/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "npm run clean && vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <20",
    "react-dom": ">=16.8.0 <20"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.6.3",
    "vite": "^7.1.7",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "neuctra",
    "authix",
    "ui-components",
    "react",
    "tailwind",
    "vite",
    "library",
    "authentication",
    "jwt",
    "otp",
    "sdk",
    "auth"
  ]
}
